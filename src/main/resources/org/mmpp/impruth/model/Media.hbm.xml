<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.mmpp.impruth.model">
  <class name="Media" table="MEDIA">
  	<id name="id" type="int" column="id">
	  	<meta attribute="scope-set">protected</meta>
  		<generator class="increment" />
  	</id>
	<property name="title" type="string" not-null="true" />
	<property name="subtitle" type="string"  />
	<property name="pretitle" type="string"  />
	<property name="volume" type="int"  />
	<property name="volumeValue" column="volume_value" type="string"  />
  </class>
</hibernate-mapping>